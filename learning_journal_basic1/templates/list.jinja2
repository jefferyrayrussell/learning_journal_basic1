{% extends "index.jinja2" %}
{% block body %}
  {% if entries %}
    {% for entry in entries %}
      <article>
        <h4><a href="{{ request.route_url('detail_view', id=entry.id) }}">{{ entry.title }}</a><br>{{ entry.date }}</h4>
      </article>
    {% endfor %}
  {% else %}
    <p>This journal entry is empty.</p>
  {% endif %}
{% endblock %}