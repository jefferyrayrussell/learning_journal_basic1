{% extends "index.jinja2" %}
{% block body %}
  {% if entries %}
    {% for entry in entries %}
      <article>
        <h2>{{ entry.date }}  <a href="{{ request.route_url('detail_view', id=entry.id) }}">{{ entry.title }}</a></h2>
      </article>
    {% endfor %}
  {% else %}
    <p>This journal entry is empty.</p>
  {% endif %}
{% endblock %}